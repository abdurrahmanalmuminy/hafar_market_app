# Example environment variables for Hafar Market App
# Copy this file to `.env` and fill in the real values.

# Algolia Search
ALGOLIA_APP_ID=YOUR_ALGOLIA_APP_ID
ALGOLIA_API_KEY=YOUR_ALGOLIA_SEARCH_API_KEY

# Moyasar Payment
MOYASAR_PUBLISHABLE_KEY=YOUR_MOYASAR_PUBLISHABLE_KEY
MOYASAR_APPLE_MERCHANT_ID=merchant.your.bundle.id

# Dynamic Links
DYNAMIC_LINK_DOMAIN=hafarmarket.page.link
APP_LINK_HOST=https://hafarmarket.app

